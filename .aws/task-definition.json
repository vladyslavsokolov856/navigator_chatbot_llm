{
    "family": "alithias-task-definition",
    "containerDefinitions": [
        {
            "name": "nginx",
            "image": "616066296376.dkr.ecr.us-east-1.amazonaws.com/nginx:b94849eb458a9246aa899268a99587ca7054dbbc",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "nginx-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "LOG_LEVEL",
                    "value": "info"
                },
                {
                    "name": "ENVIRONMENT",
                    "value": "prod"
                }
            ],
            "environmentFiles": [
                {
                    "value": "arn:aws:s3:::alithias-gpt-app/.env",
                    "type": "s3"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "dependsOn": [
                {
                    "containerName": "next-js",
                    "condition": "START"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/alithias-task-definition",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        },
        {
